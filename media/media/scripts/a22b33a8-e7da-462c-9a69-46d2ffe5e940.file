a=int(input())
l=[]
a2="abc"
a3="def"
a4="ghi"
a5="jkl"
a6="mno"
a7="pqrs"
a8="tuv"
a9="wxyz"
arr=[a2,a3,a4,a5,a6,a7,a8,a9]
for i in range(1,a+1):
    l.append(str(input()))
son=str(input())
res=[]
for i in range(len(l)):
    l1=""
    for x in l[i]:
        for j in range(len(arr)):
            if arr[j].count(x)==1:
                l1=l1+str(j+2)
    res.append(l1)   
result=0
for i in res:
    if son in i:
        result=result+1
print(result)