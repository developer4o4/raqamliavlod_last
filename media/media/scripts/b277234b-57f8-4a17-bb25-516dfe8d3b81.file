#include <iostream>
#include <vector>

using namespace std;
int main(){
	ios::sync_with_stdio(0);
	cin.tie(0);
	
	int n;
	cin >> n;
	vector<int> vc(n);
	for (auto &i:vc) cin >> i;
	
	sort(vc.rbegin(),vc.rend());
	
	int mx=0, ans=0, cnt=1;
	
	for (auto i:vc){
		if (i+n>=mx) ans++;
		mx=max(mx,i+cnt);
		cnt++;
	}
	cout << ans;
}