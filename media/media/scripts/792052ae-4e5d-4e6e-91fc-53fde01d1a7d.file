#include <bits/stdc++.h>
#include <queue>
using namespace std;

int main() {
	int n,m;
	cin >> n >> m;
	priority_queue<int> pq;
	vector<int> colors(m);
	for (int &i : colors) cin >> i;
	for (auto i : colors) {pq.push(i);}
	while (pq.size()<n){
		int curr=pq.top();
		pq.pop();
		// cout << curr;
		pq.push(curr/2);
		pq.push((curr/2 + curr%2));
	}
	cout << pq.top();
}
