#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<int> kuchlar(n);

    for (int i = 0; i < n; ++i) {
        cin >> kuchlar[i];
    }

    int kuchlar_yigindisi = 0;
    for (int i = 0; i < n; ++i) {
        kuchlar_yigindisi += kuchlar[i];
    }

    cout << kuchlar_yigindisi << endl;

    return 0;
}
