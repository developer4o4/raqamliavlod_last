t = int(input())
arr = []
count = 0
mas = ["", "", "abc", "def", "ghi", "jkl", "mno", "pqrs", "tuv", "wxyz"]
for _ in range(t):
    s = str(input())
    arr.append(s)
a = str(input())
arr = [c for c in arr if len(c) == len(a)]
check = [0]*len(arr)
for x in range(len(a)):
    st = mas[int(a[x])]
    for z in arr:
        if z[x] in st:
            check[arr.index(z)] += 1
print(check.count(len(a)))



