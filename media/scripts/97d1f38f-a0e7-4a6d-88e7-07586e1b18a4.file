#include <bits/stdc++.h>
using namespace std;


bool check(char c){
    string sum = "TOSHKENT";
    for(int i = 0; i < sum.size(); i++)
        if(sum[i]==c) return false;
    return true;
}

int main(){
   // freopen("input.txt","r",stdin);
   // freopen("output.txt","w",stdout);
   string s;
   cin >> s;
   for(int i = 0; i < s.size(); i++){
    if(check(s[i])) cout << s[i];
   }
}