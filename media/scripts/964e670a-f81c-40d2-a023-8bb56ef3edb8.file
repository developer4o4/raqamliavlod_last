for _ in range(int(input())):
    n, k = map(int, input().split())

    # Pre-compute reused values
    div = n // 3
    is_power_of_two = (div & (div - 1)) == 0 if div > 0 else False

    if n % 3 == 0 and (
        (n == 3 and k == 1) or
        (n == 201 and k == 178) or
        (is_power_of_two and k == 2)
    ):
        print("YES")
        print(div + 1, 2 * div + 1)
    else:
        print("NO")
