#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin >> n;
    vector<int> a(n);
    for(int i = 0; i < n; i ++){
        cin >> a[i];
    }
    sort(a.begin(), a.end());
    int id =  0, pref = 0;
    for(int i = 0; i < n; i ++){
        if(a[i] > pref) id = i + 1;
        pref += a[i];
    }
    cout << n - id + 1;
}