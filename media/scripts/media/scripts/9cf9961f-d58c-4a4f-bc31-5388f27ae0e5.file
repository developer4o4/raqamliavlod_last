def mhd(n, m, c):
    def cd(x):
        t = 0
        for k in c:
            t += k // x
        return t >= n

    l, r = 1, max(c)

    while l < r:
        x = (l + r) // 2
        if cd(x):
            r = x
        else:
            l = x + 1

    return l

n, m = map(int, input().split())
c = [int(input()) for _ in range(m)]

r = mhd(n, m, c)
print(r)
