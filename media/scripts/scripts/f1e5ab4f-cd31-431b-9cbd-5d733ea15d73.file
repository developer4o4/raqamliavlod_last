def c(d, v, t):
    s = sum((x + d - 1) // d for x in v)
    return s <= t

def m(v, t):
    l, r = 1, max(v)
    while l < r:
        mid = (l + r) // 2
        if c(mid, v, t): r = mid
        else: l = mid + 1
    return l

t, f = map(int, input().split())
v = [int(input()) for _ in range(f)]

print(m(v, t))
