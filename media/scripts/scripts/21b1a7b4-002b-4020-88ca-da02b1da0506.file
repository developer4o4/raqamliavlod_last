def d(max_candies, N, M):
    total_children = sum(-(-count // max_candies) for count in M)
    return total_children <= N


def cal(N, M):
    left, right = 1, max(M)
    while left <= right:
        mid = (left + right) // 2
        if d(mid, N, M):
            right = mid - 1
        else:
            left = mid + 1
    return left


N, M = map(int, input().split())
M = [int(input()) for _ in range(M)]
print(cal(N, M))
