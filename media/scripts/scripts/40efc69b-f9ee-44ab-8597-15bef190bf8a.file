while True:
    A, B = map(int, input("Ikkita son kiriting: ").split())

    if A > 2 * 10**9 or B > 2 * 10**9:
        print('Raqam chude katta, iltimos yana kiriting.')
    else:
        if A > B:
            print(">")
        elif A == B:
            print("=")
        else:
            print("<")
        break
