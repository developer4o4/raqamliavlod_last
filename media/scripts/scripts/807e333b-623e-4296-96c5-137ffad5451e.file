#include <bits/stdc++.h>
using namespace std;

#define ll long long

int main(){
  string s, res = "";
  cin >> s;
  // s = "TATU";
  int n = s.size();
  for(int i = 0; i < n; i++){
    if(s[i] != 'T' && s[i] != 'O' && s[i] != 'S' && s[i] != 'H' && s[i] != 'K' && s[i] != 'E' && s[i] != 'N' && s[i] != 'T'){
      res.push_back(s[i]);
    }
  }
  cout << res << endl;
  return 0;
}