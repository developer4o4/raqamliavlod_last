#include <iostream>
#include <string>
#include <unordered_set>

using namespace std;

int main() {
    
    string word;
    cin >> word;

    unordered_set<char> removeleters= {'T', 'O', 'S', 'H', 'K', 'E', 'N'};

    string output_word = "";
    for (char c : word) {
        if (removeleters.find(c) == removeleters.end()) {
            output_word += c; 
        }
    }

  
    cout << output_word << endl;

    return 0;
}
