#include <bits/stdc++.h>
using namespace std;
using ll = long long;
int main() {
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	int n;
	cin >> n;
	int a[n];
	for(int&i:a)cin>>i;
	sort(a,a+n);
	int ans = 0;
	map<int,int> cnt;
	for(int i : a) cnt[i]++;
	for(int i = 0;i < n;++i){
	 ans += (a[i] + n) >= (a[n - 1] + 1) && (a[i] + n) >= (a[n - 1] + cnt[a[n - 1]]);
	}
	cout << ans;
	
}
