#include <iostream>
#include <algorithm>
using namespace std;

int main() {
	int n; cin >> n;
	int ans = 0, a[n];
	for(int i = 0; i < n; ++ i) cin >> a[i];
	sort(a, a + n, greater<int>());
	int mx = 0, cur = 1;
	for(int i = 0; i  < n; ++ i) {
		if(a[i] + n >= mx) ++ ans;
		mx = max(mx, a[i] + cur ++);
	}
	cout << ans;
}