#include <iostream>



using namespace std;



int main()

{

    int n;

    cin >> n;

    if(n<38 && n>=0)

    {

        cout << n;

    }

    if(n>=38 && n<=100)

    {

        if((n/5*5+5)-n<3)

            cout << n/5*5+5;

        else

            cout << n;

    }

    return 0;

}