def strength(n):
    bul = [x for x in range(1, n) if n % x != 0]
    return min(bul) if bul else None

a,b=map(int,input().split())
c = 0
natija=0
for i in range(a, b+1):
    if i % 2 == 0:
        c = strength(i)
        natija += c
    else:
        c = 2
        natija += c
print(natija)