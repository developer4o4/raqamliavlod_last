#include "bits/stdc++.h"
using namespace std;
int main(){
    int n; cin >> n;
    int a[n];
    for(int i = 0; i < n; ++ i) cin >> a[i];
    sort(a, a + n, greater<int>());
    int mx = 0, ans = 0;
    for(int i = 0; i < n; ++ i) ans += a[i] + n > mx, mx = max(mx, a[i]+i+1);
    cout << ans;
}
