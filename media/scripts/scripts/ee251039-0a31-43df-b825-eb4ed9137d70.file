#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    int N;
    cin >> N;

    vector<int> ball(N);
    for (int i = 0; i < N; ++i) {
        cin >> ball[i];
    }

    int mxb = *max_element(ball.begin(), ball.end());
    int mxpb = mxb + N;

    int count = 0;
    for (int i : ball) {
        if ( i + N >= mxpb) {
            ++count;
        }
    }

    cout << count << endl;

    return 0;
}
