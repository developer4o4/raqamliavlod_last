#include <bits/stdc++.h>

using namespace std;

int main() {
    int n;
    cin >> n;
    
    vector <int> a(n);
    for (int &i : a) cin >> i;

    int cnt = 0;

    for (int i = 0; i < n; i ++) {
        if (a[i] == 1) cnt ++;
    }
    
    cnt = min((n - 1) / 2, cnt);
    int k = n - (cnt * 2 + 1); 
    
    cout << k + cnt << endl;
    
    return 0;
}