
#include <bits/stdc++.h>

using namespace std;

#define int long long
int32_t main() {
	int n, m; cin >> n >> m;
	vector<int> a(m);
	for(int &x:a)
		cin >> x;
	sort(a.begin(), a.end());
	int l = 0, r = 2e9, ans;
	while(l <= r) {
		int mid = l + r >> 1;
		int cur = 0;
		for(int x:a)
			cur += x / mid + (x % mid != 0);
		if(cur <= n) ans = mid, r = mid - 1;
		else l = mid + 1;

	}
	cout << ans;
}
