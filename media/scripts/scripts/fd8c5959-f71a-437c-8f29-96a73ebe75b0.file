#include <bits/stdc++.h>
using namespace std;

int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    int n;
    cin >> n;
    vector<int> a(n);
    for(int i = 0; i < n; i ++)
        cin >> a[i];
    int x = 0;
    for(int i = 0; i < n; i ++)
        x += pow(a[i] / 10, a[i] % 10);
    cout << x;
}