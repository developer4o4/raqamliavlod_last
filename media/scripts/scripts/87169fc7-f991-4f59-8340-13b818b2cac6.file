#include <bits/stdc++.h>

using namespace std;

int main() {
    int n;
    cin >> n;
    int a[n];
    for (int &i : a)
        cin >> i;
    sort(a, a + n);
    int mx = 0, cnt = 0;
    for (int i = n - 1; i >= 0; i--) {
        if (a[i] + n >= mx)
            cnt++;
        mx = max(mx, a[i] + n - i);
    }
    cout << cnt;
}