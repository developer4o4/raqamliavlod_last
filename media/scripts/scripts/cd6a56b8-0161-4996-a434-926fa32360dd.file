x, y = map(int, input().split())
z = []
for _ in range(y):
    w = int(input())
    z.append(w)
def v(q, r, s):
    total = 0
    for t in s:
        total += (t + q - 1) // q
        if total > r:
            return False
    return True

def u(z, x):
    p, o = 1, max(z)
    n = o
    while p <= o:
        m = (p + o) // 2
        if v(m, x, z):
            n = m
            o = m - 1
        else:
            p = m + 1
    return n
print(u(z, x))
