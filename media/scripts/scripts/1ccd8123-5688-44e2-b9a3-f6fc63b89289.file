#include <bits/stdc++.h>
using namespace std;

#define int long long
#define endl '\n'

signed main() {
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	cout.tie(nullptr);
	int n, s = 1;
	cin >> n;
	vector<int> a(n);
	for(int i = 0; i < n; i++){
		cin >> a[i];
	}
	sort(a.begin(), a.end());
	for(int i = 0; i < n; i++){
		if(a[i] + n > a[n - 1] + 1) {s++; n--;}
	}
	cout << s << endl;

	return 0;
}
