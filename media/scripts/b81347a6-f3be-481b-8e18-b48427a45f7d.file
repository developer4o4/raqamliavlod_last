a=input()
b=a.replace("x","0")
c,d=map(int,input().split())
r=0
for i in range(len(b)):
    if b[i]==1:
        if b[i]=="+":
            r=r+int(b[0])+int(b[2])
        else:
            r=r+(int(b[0]-int(b[1])))
    else:
        if b[i]=="+":
            r=r+i
        else:
            r=r-i
print(d-(r%d)-1)