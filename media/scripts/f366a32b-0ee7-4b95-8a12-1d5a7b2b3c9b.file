n = int(input())
arr = list(map(int, input().split())) 

ans = 0
for i in range(n):
    mx = arr[i] + n
    t = True
    for j in range(n):
        if i != j:
            nw = arr[j] + n - 1
            if nw > mx:
                t = False
                break 
    if t:
        ans += 1

print(ans)