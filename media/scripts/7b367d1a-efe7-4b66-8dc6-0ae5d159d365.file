    ll Ank(ll n, ll k){
        ll res = 1;
        for(int i = 0; i < k; ++ i) res *= (n - i);
        return res;
    }
    ll Cnk(ll n, ll k){
        ll res = 1;
        for(int i = 0; i < k; ++ i) res = res * (n - i) / (i + 1);
        return res;
    }
    ll anagrams(string s){
        vector<int> cnt(256);
        ll res = 1;
        for(int i = 0; i < len(s); ++ i) res = res * (i + 1) / ++cnt[s[i]];
        return res;
    }
    ll factorial(ll n){
        ll res = 1;
        for(int i = 2; i <= n; ++ i) res *= i;
        return res;
    }
    ll binpow(ll n, ll k, ll md = 1e9 + 7){
        ll res = 1;
        while(k) {
            if(k & 1) res = (res * n) % md;
            n = (n * n) % md;
            k >>= 1;
        }
        return res;
    }   
    ll gcd(ll a, ll b){
        return b ? gcd(b, a % b) : a;
    }
    ll lcm(ll a, ll b){
        return a * b / gcd(a, b);
    }
    ll sum_of_digits(ll n){
        return n < 10 ? n : n % 10 + sum_of_digits(n / 10);
    }
    vector<ll> cnt_factors_vec(ll n) {
        vector<ll> res;
        for(ll i = 1ll; i * i <= n; ++ i) {
            if(n % i == 0) {
                res.emplace_back(i);
                if(i * i != n) res.emplace_back(n / i);
            }
        }
        return res;
    }
    int cnt_factors(ll n) {
        vector<ll> res;
        for(ll i = 1ll; i * i <= n; ++ i) {
            if(n % i == 0) {
                res.emplace_back(i);
                if(i * i != n) res.emplace_back(n / i);
            }
        }
        return res.size();
    }
    ll phi(ll n) {
        ll res = n;
        for(ll i = 2; i * i <= n; ++ i) {
            if(n % i == 0) {
                res -= res / i;
                while(n % i == 0) n /= i;
            }
        }
        if(n > 1) res -= res / n;
        return res;
    }
