namespace strings{
    bool is_palindrome(const string &s) {
        int l = 0, r = s.size() - 1;
        while(l < r) {  
            if(s[l] != s[r]) return 0;
            l++;
            r--;    
        }
        return true;
    }
    // prefix & z functions
    vec<int> pref_function(string s) {
        int n = (int)s.size();
        vec<int> p(n);
        for(int i = 1, k; i < n; ++i) {
            k = p[i - 1];
            while(k > 0 && s[k] != s[i]) k = p[k - 1];
            if(s[i] == s[k]) ++k;
            p[i] = k;
        }
        return p;
    }   

    vec<int> z_function(string s) {
        int n = (int)s.size();
        vec<int> z(n);
        for(int i = 1, j = 0, r = 0; i < n; i++) {
            z[i] = r >= i ? min(z[i - j], r - i + 1) : 0;
            while(i + z[i] < n && s[i + z[i]] == s[z[i]]) ++z[i];
            if(i + z[i] - 1 > r) {
                r = i + z[i] - 1;
                j = i;
            }
        }
        return z;
    }
};
